{
    "aws.logLevel": "info",
    "aws.onDefaultRegionMissing": "ignore",
    "aws.samcli.location": "C:\\Program Files\\Amazon\\AWSSAMCLI\\bin\\sam.cmd",
    "aws.telemetry": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.codeLens": true,
    "editor.detectIndentation": false,
    "editor.fontSize": 12,
    "editor.insertSpaces": true,
    "editor.suggestFontSize": 12,
    "editor.tabSize": 4,
    "explorer.confirmDelete": false,
    "files.autoSave": "afterDelay",
    "files.trimTrailingWhitespace": true,
    "python.autoUpdateLanguageServer": true,
    "python.languageServer": "Pylance",
    "python.linting.enabled": true,
    "python.linting.pylintArgs": [
        "--enable=W0614",
        "--logging-format-style=fstr"
    ],
    "python.linting.pylintEnabled": true,
    "search.useGlobalIgnoreFiles": true,
    "shellLauncher.shells.windows": [
        {
            "shell": "C:\\Windows\\System32\\cmd.exe",
            "label": "Cmd"
        },
        {
            "shell": "C:\\Program Files\\Git\\bin\\bash.exe",
            "label": "Git bash"
        },
        {
            "shell": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
            "label": "PowerShell"
        },
        {
            "shell": "C:\\Windows\\System32\\bash.exe",
            "label": "WSL Bash"
        }
    ],
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\wsl.exe",
    "window.zoomLevel": -1,
    // https://code.visualstudio.com/api/references/theme-color
    "workbench.colorCustomizations": {
        "tree.indentGuidesStroke": "#fffb04",
        "[Monokai Dimmed]": {
            "editor.background": "#000000",
            "sideBar.background": "#000000"
        },
        "[Night Owl (No Italics)]": {
            "terminal.ansiGreen": "#497a49"
        }
    },
    "workbench.colorTheme": "Night Owl (No Italics)",
    "workbench.preferredDarkColorTheme": "Monokai Dimmed",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.tree.indent": 20,
    "workbench.tree.renderIndentGuides": "always",
    "yaml.customTags": [
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
        "!Equals sequence",
        "!Or",
        "!Or sequence",
        "!FindInMap",
        "!FindInMap sequence",
        "!Base64",
        "!Join",
        "!Join sequence",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!Sub sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!ImportValue sequence",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Split sequence"
    ],
    "yaml.validate": false,
    "[bat]": {
        "editor.tabSize": 2
    },
    "[dockerfile]": {
        "editor.tabSize": 2
    },
    "[groovy]": {
        "editor.tabSize": 4
    },
    "[html]": {
        "editor.tabSize": 2
    },
    "[javascript]": {
        "editor.tabSize": 2
    },
    "[javascriptreact]": {
        "editor.tabSize": 2
    },
    "[json]": {
        "editor.tabSize": 4
    },
    "[powershell]": {
        "editor.tabSize": 2
    },
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "[shellscript]": {
        "editor.tabSize": 2
    }
}