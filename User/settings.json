{
    "aws.logLevel": "info",
    "aws.samcli.enableCodeLenses": false,
    "aws.samcli.location": "C:\\Program Files\\Amazon\\AWSSAMCLI\\bin\\sam.cmd",
    "aws.telemetry": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.acceptSuggestionOnEnter": "on",
    "editor.codeLens": true,
    "editor.detectIndentation": false,
    "editor.fontSize": 12,
    "editor.insertSpaces": true,
    "editor.quickSuggestions":{
        "comments": "on",
        "strings": "on",
        "other": "on"
    },
    "editor.snippetSuggestions":"top",
    "editor.suggest.preview": true,
    "editor.suggest.showMethods": true,
    "editor.suggestFontSize": 12,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "on",
    "editor.tabSize": 4,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.autoSave": "afterDelay",
    "files.trimTrailingWhitespace": true,
    "go.lintOnSave": "off",
    "go.toolsManagement.autoUpdate": true,
    "json.schemas": [],
    "markdown.preview.fontSize": 13,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "python.languageServer": "Pylance",
    "python.linting.enabled": true,
    "python.linting.pylintArgs": [
        "--enable=W0614",
        "--logging-format-style=fstr"
    ],
    "python.linting.pylintEnabled": true,
    "redhat.telemetry.enabled": false,
    "scm.inputFontSize": 12,
    "search.useGlobalIgnoreFiles": true,
    // https://code.visualstudio.com/docs/editor/workspace-trust
    "security.workspace.trust.enabled": false,
    "security.workspace.trust.startupPrompt": "never",
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.defaultProfile.windows": "Ubuntu-20.04 (WSL)",
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        },
        "Ubuntu-20.04 (WSL)": {
            "path": "C:\\WINDOWS\\System32\\wsl.exe"
        }
    },
    // https://code.visualstudio.com/api/references/theme-color
    "workbench.colorCustomizations": {
        "tree.indentGuidesStroke": "#fffb04",
        "[Monokai Dimmed]": {
            "editor.background": "#000000",
            "sideBar.background": "#000000"
        },
        "[Night Owl (No Italics)]": {
            "terminal.ansiGreen": "#497a49"
        }
    },
    "workbench.colorTheme": "Night Owl (No Italics)",
    "workbench.preferredDarkColorTheme": "Monokai Dimmed",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.tree.indent": 20,
    "workbench.tree.renderIndentGuides": "always",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "yaml.customTags": [
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
        "!Equals sequence",
        "!Or",
        "!Or sequence",
        "!FindInMap",
        "!FindInMap sequence",
        "!Base64",
        "!Join",
        "!Join sequence",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!Sub sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!ImportValue sequence",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Split sequence"
    ],
    "yaml.schemas": {
    },
    "yaml.validate": false,
    "[bat]": {
        "editor.tabSize": 2
    },
    "[css]": {
        "editor.tabSize": 2
    },
    "[dockerfile]": {
        "editor.tabSize": 2
    },
    "[go]": {
        "editor.formatOnSave": false,
        "editor.insertSpaces": true,
        "editor.tabSize": 4
    },
    "[groovy]": {
        "editor.tabSize": 4
    },
    "[html]": {
        "editor.tabSize": 2
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 2
    },
    "[javascriptreact]": {
        "editor.tabSize": 2
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.tabSize": 2
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[powershell]": {
        "editor.tabSize": 2
    },
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "[shellscript]": {
        "editor.tabSize": 2
    },
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "window.zoomLevel": -1
}